{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "register_command.json",
  "title": "Register command",
  "description": "A command to register a new user account.",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "password": {
      "type": "string"
    },
    "email": {
      "type": "string"
    }
  },
  "required": ["name", "password", "email"]
}